<%= form_for :user do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name, placeholder: 'Enter your username'%>
  <%= form.label :email %>
  <%= form.email_field :email, placeholder: "Enter your email" %>
  <%= form.label :password_digest %>
  <%= form.password_field :password_digest, placeholder: "Enter your password" %>
  <%= form.submit "Signup" %>
<% end %>